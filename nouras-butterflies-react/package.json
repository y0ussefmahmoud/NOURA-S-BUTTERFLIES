{
  "name": "nouras-butterflies-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "*.styl",
    "*.module.css",
    "*.module.scss",
    "*.module.sass",
    "*.module.less",
    "*.module.styl"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prepare": "husky",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "build:staging": "tsc -b && vite build --mode staging",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md,json}\"",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test:build": "npm run build && npm run preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "validate:translations": "node scripts/validate-translations.js"
  },
  "dependencies": {
    "@sentry/react": "^8.55.0",
    "@sentry/tracing": "^7.120.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.0.5",
    "@types/validator": "^13.15.10",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.3.1",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^16.5.3",
    "react-router-dom": "^6.28.0",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^2.0.1",
    "validator": "^13.15.26",
    "web-vitals": "^3.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^1.8.8",
    "@types/rollup-plugin-visualizer": "^4.2.4",
    "@vheemstra/vite-plugin-imagemin": "^2.2.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.0",
    "tailwindcss-rtl": "^0.9.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-compression2": "^2.4.0",
    "vitest": "^1.2.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
